     "text": [
      "a = 4\n",
      "b = 7\n",
      "a = 3\n"
     ]
    }
   ],
   "source": [
    "a = 7 # 0111\n",
    "b = 3 # 0011\n",
    "\n",
    "# 两者相等为0,不等为1.\n",
    "\n",
    "a = a ^ b # 0111 ^ 0011 = 0100 == 4\n",
    "print(\"a = {}\".format(a))\n",
    "b = a ^ b # 0100 ^ 0011 = 0111 == 7\n",
    "print(\"b = {}\".format(b))\n",
    "a = a ^ b # 0100 ^ 0111 = 0011 == 3\n",
    "print(\"a = {}\".format(a))\n"
   ]
